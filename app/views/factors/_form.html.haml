= render partial: "shared/print_errors", locals: {obj: @factor}
= form_for @factor do |f|
  .form-group
    %label Priority
    = f.grouped_collection_select :dimention_id, Priority.all, :dimentions, :title, :id, :title, :class => "form-control"
  .form-group
    %label Title
    = f.text_field :title, :class => "form-control"
  .form-group
    %label Weight
    = f.number_field :weight, :class => "form-control"
  .form-group
    = f.submit class: "button btn btn-primary"
